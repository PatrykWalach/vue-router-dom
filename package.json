{
  "name": "vue-router-dom",
  "version": "0.4.1",
  "description": "Router for Vue 3 and up",
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-standard-pkg",
        {
          "exclude": [
            "__tests__/**/*"
          ]
        }
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-build-web"
      ],
      [
        "@pika/plugin-build-types"
      ]
    ]
  },
  "scripts": {
    "build": "npx @pika/pack",
    "coverage": "jest --collect-coverage",
    "codecov": "codecov",
    "lint": "eslint --fix src/**/*.ts{,x}",
    "test": "jest",
    "watch": "jest --watch"
  },
  "keywords": [
    "vue",
    "composition-api",
    "router",
    "hooks",
    "typescript"
  ],
  "author": "Patryk Wa≈Çach",
  "license": "MIT",
  "peerDependencies": {
    "vue": "^3.0.0-alpha.0"
  },
  "devDependencies": {
    "@pika/pack": "^0.5.0",
    "@pika/plugin-build-node": "^0.9.2",
    "@pika/plugin-build-types": "^0.9.2",
    "@pika/plugin-build-web": "^0.9.2",
    "@pika/plugin-standard-pkg": "^0.9.2",
    "@types/jest": "^25.1.2",
    "@types/node": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "codecov": "^3.6.5",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.0",
    "typescript": "^3.8.2",
    "vue": "^3.0.0-alpha.6"
  },
  "dependencies": {
    "history": "^4.10.1",
    "path-to-regexp": "^6.1.0"
  }
}
