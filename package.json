{
  "name": "vue-router-dom",
  "version": "1.0.0-beta.1",
  "description": "Router for Vue 3 and up",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "vite build",
    "typecheck": "vue-tsc --noEmit",
    "prepublishOnly": "npm run build",
    "codecov": "codecov",
    "test": "vitest",
    "ct:test": "cypress run --component",
    "ct:dev": "cypress open --component",
    "prepare": "husky install"
  },
  "keywords": [
    "vue",
    "composition-api",
    "router",
    "hooks",
    "typescript"
  ],
  "author": "Patryk WaÅ‚ach",
  "license": "MIT",
  "peerDependencies": {
    "vue": ">=3.0.0-alpha.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PatrykWalach/vue-router-dom.git"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.0.1",
    "@vitejs/plugin-vue-jsx": "^2.0.0",
    "@vue/test-utils": "^2.0.2",
    "c8": "^7.12.0",
    "cypress": "^10.3.1",
    "happy-dom": "^6.0.4",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.4",
    "vite-plugin-dts": "^1.4.0",
    "vitest": "^0.19.1",
    "vue": "^3.2.37",
    "vue-tsc": "^0.39.2"
  },
  "dependencies": {
    "@remix-run/router": "^0.2.0-pre.5"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": "prettier --write"
  }
}
